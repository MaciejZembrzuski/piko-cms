<html>
<head>
<meta http-equiv="content-type" content="text/html; charset=utf-8" />
<title>{TITLE}</title>
<meta name="description" content="{DESC}" />
<link rel="stylesheet" type="text/css" href="default.css" />
</head>
<body>

<div id="outer">

	<div id="upbg"></div>

	<div id="inner">

		<div id="header">
			<h1>{H1}</h1>
			<h2>{H2}</h2>
		</div>
	
		<div id="splash"></div>
	
		<div id="menu">
		</div>
	

		<div id="primarycontent">
		
			<!-- primary content start -->
			<div class="post">
				<div class="content">
					{CONTENT}
				</div>			
			</div>
			<!-- primary content end -->
	
		</div>
		
		<div id="secondarycontent">

			<!-- secondary content start -->
			<div class="content">
				{SIDEBAR}
			</div>
			
			<div class="content">
				<h3>Podstrony</h3>
				<ul class="linklist">
					{LINKS}
				</ul>
			</div>

			<!-- secondary content end -->

		</div>
	
		<div id="footer">
		
			{FOOTER} CMS: <a href="http://www.fabrykaspamu.pl/pikocms/">PikoCMS</a>, grafika: <a href="http://www.nodethirtythree.com/">NodeThirtyThree</a> + <a href="http://www.freecsstemplates.org/">Free CSS Templates</a>.
		
		</div>

	</div>

</div>

</body>
</html>
